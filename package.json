    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "forever": "^0.15.3",
    "postcss": "^5.2.11",
    "postcss-color-hex-alpha": "^2.0.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "clean": "rimraf public",
    "copy:html": "mkdir -p public && cp -a src/server/index.html public/",
    "copy": "npm run copy:html",
    "build:webpack": "`npm bin`/webpack",
    "build": "npm run clean && npm run copy && npm run build:webpack",
    "watch:html": "cpx --watch src/server/index.html public",
    "watch:webpack": "`npm bin`/webpack -w",
    "watch": "npm run watch:html & npm run watch:webpack",
    "start": "npm run build && npm run watch & `npm bin`/nodemon --exec node -r babel-register src/server/index.js --ignore logs/* ",
    "dev:start": "docker-compose -f docker-compose.yml -p yubiori up -d --build",
    "dev:kill": "docker-compose -f docker-compose.yml -p yubiori kill",
    "dev:destroy": "docker-compose -f docker-compose.yml -p yubiori rm -f",
    "dev:ps": "docker-compose -f docker-compose.yml -p yubiori ps",
    "dev:logs": "docker-compose -f docker-compose.yml -p yubiori logs --follow --timestamps",
    "dev:restart": "npm run dev:kill && npm run dev:destroy && npm run dev:start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "k.tada",
  "license": "MIT"
}
